subroutines {
   add_point : subroutine {
      : print "<Your score has just gone up by a point!<#ff0>>" ;
      : increment "score" ;
      : update_graphic ;
   }

   check_options : subroutine {
      : match "hone aye;hone eye" {
         : print {( "Available exits are " + (opt_hone_aye ? "already" : "now") + " honed to ones you still need to use to win." )} ;
         : if (clue_number == 0 && !opt_hone_aye) {
            : print "Since you're choosing this option, and you're done here, I'll make a note of that. You can exit, but you can't come back." ;
         }
         : set_true "opt_hone_aye" ;
      }
      : match "ho nigh" {
         : print {( "Exits are " + (opt_hone_aye ? "already" : "now") + " all available, even ones you don't need to revisited." )} ;
         : set_false "opt_hone_aye" ;
      }
   }

   note_thinks : subroutine {
      : if (!think_note_yet) {
         : print "{note_once}{v_think} will track any command that will score points at a later time. Also, the status line in the upper left will show +1 or more, for commands you've guessed right but still need prep for." ;
         : set_true "think_note_yet" ;
      }
   }
   say_half : subroutine {
      : print "You got half right." ;
   }
}

themes {
   my_theme : theme {
      status_bar {
         : header_text ;
         : dynamic_text "my_score" ;
      }
      theme_settings {
         parser_delay  = 0
      }
      colors {
         status_bar_pen   = 15
         status_bar_paper = #222
      }
      system_messages {

   i_cant_do_that = That's a valid command, but not right here and now.
   not_present = You can't see ${entity} here.
   ask_new_game                                   = Would you like to restart {cdsa}?
   ask_quit                                       = Do you wish to quit {cdsa}?
   invalid_choice                                 = {yn_or_12_or_14}
   post_quit = I hope you enjoyed the experience. Pressing {key_enter} will restart {cdsa}.
   unknown_verb                                   = {custom_unknown_verb_error}
   you_cant_go_that_direction                     = {exits_err}
   you_see_nothing_special = {nospecial_custom}
   you_see_nothing_special_2 = {nospecial_custom}
      }
   }
}

## Print the objective when the game starts

on_startup {
   : print "{cdsa}: {ecomp} 2025 Petite Mort game" ;
   : press_any_key ;
   : print "Halloween. You want to avoid kids trick-or-treating. You cut through a park you've never seen before." ;
   : update_graphic "s_start_1" ;
   : print "No-spark-knows park? It's dark in there ... but wait ... there's a light ahead, or something giving light ..." ;
   : press_any_key ;
   : update_graphic "s_start_2" ;
   : print "A huge glowing ark appears before you. A voice intones: CHEZ DARK SHADE ARK. You feel compelled to enter ..." ;
   : press_any_key ;
   : print "And when you do, unseen hands grab you. \"The first visitor through the portal! They don't look suspicious at all, but remember, it's the people we least suspect that are ruining things!\"" ;
   : print "You black out and wake up in ...";
   : press_any_key ;
   : clear_screen;
}

## Execute this every time a player moves or redescribes a location

on_describe {
   : if (!intro_shown_yet) {
      : print "{note_once}{cdsa} uses a reduced parser. {v_verbs} details them, though standard starting commands like {v_about} and {xme} may help you get started." ;
      : set_true "intro_shown_yet" ;
   }
}

## Test VERB NOUN that the player types and executes conditional actions

on_command {
   : match "xyzzy _" {
      : print "A hollow voice intones \"Our gaol: argh, ail!\" It's so spooky, you don't dare quibble over details of pronunciation." ;
      : done ;
   }
   : mask {
      : match "load -" {
         : print "You hear a low \"Do load, ooh!\"" ;
      }
      : match "restore -" {
         : print "You hear a tune. It could be called a re-load reel/ode, I guess." ;
      }
      : match "save -" {
         : print "A ghostly apparition with \"Save? Illun', Say!\" Villain written on its front taunts your very reasonable precaution. But you don't need to listen to such meanies." ;
      }
   }
   : match "z _;wait _" {
      : print "\"Loaf, right? Lo, fright!\"" ;
      : if (!wait_yet) {
         : print "{note_once}This is just a silly general response. Waiting gives no penalty or benefit." ;
         : set_true "wait_yet" ;
      }
   }
   : match "map _;m _" {
      : set_true "showing_map" ;
      : clear_screen ;
      : update_graphic "m_blank" ;
      : if (!first_map_note) {
         : print "{note_once}You can mouse-click or hit {key_enter} to return to the game. Arrows indicate one-way paths, and red lines show ones you don't need any more, if {hone_aye} is active." ;
         : set_true "first_map_note" ;
      }
      : press_any_key ;
      : set_false "showing_map" ;
      : redescribe ;
   }
   : if (pawn_is_around) {
      : match "_ pawn" {
         : print "The pawn is keeping low until the moment it needs to attack." ;
         : done ;
      }
   }
   : if (is_carried "aid_art") {
      : match "dart -" {
         : if (point_a_dart) {
            : print "Already done!" ;
            : done ;
         }
         : destroy "aid_art" ;
         : pocket "dart" ;
         : print "The aid art doesn't have explicit instructions, how to create a dart, but you suppose that's for the best. People shouldn't have too-easy access to lethal weapons. It takes a while to figure what to do, but thanks to your earlier wordsmithing, you see methods others might have missed. Enough to create a dart!" ;
         : set_true "point_a_dart" ;
         : gosub "add_point" ;
         : done ;
      }
      : if (!point_a_dart) {
         : match "dart _;_ dart" {
            : print "You just technically need one word, due to how Adventuron processes certain words." ;
            : done ;
         }
      }
   }
   : if (is_carried "key_nigh" && is_carried "sly_maps") {
      : match "x key; x map;x maps;x sly;x nigh" {
         : print "You notice the key and the map come together to form a full maze. This could be useful later!" ;
         : set_false "showing_room" ;
         : update_graphic "s_maze_full" ;
         : set_true "showing_room" ;
         : set_true "progress_full_map" ;
         : done ;
      }
   }
   : if (is_carried "key_nigh") {
      : match "x key;x nigh" {
         : print "You see the bottom half of a maze." ;
         : set_false "showing_room" ;
         : update_graphic "s_maze_s" ;
         : set_true "showing_room" ;
         : done ;
      }
   }
   : if (is_carried "sly_maps") {
      : match "x map;x maps;x sly" {
         : print "You see the top half of a maze." ;
         : set_false "showing_room" ;
         : update_graphic "s_maze_n" ;
         : set_true "showing_room" ;
         : done ;
      }
   }
   : if (point_thee_floored) {
      : match "hi strawberry" {
         : print "You look around to find someone or something or somefruit to notify of the heist robbery, but they're not here." ;
         : set_true "think_hi_strawberry" ;
         : gosub "note_thinks" ;
         : done ;
      }
      : if (!point_hi_strawberry) {
         : match "hi _;_ strawberry" {
            : gosub "say_half" ;
            : done ;
         }
      }
   }

   : gosub "check_options" ;
   : match "about _" {
      : print "{cdsa} is a simplified-parser game that requires two-word commands to solve puzzles. Some puzzles rely on others. A few have alternate spellings. One, due to Adventuron's parser, can eliminate the first (very common) word. {cdsa} should always say if you are half-right." ;
      : print "" ;
      : print "{cdsa}'s main area is 3x3, with the start and end rooms falling outside. Originally I wanted to make it symmetrical, but unfortunately the clearest way to do so would draw a hate symbol. You know which one. So if you want to map {cdsa} on your own, the left 2x3 are ground level, and the first room is center west." ;
      : print "" ;
      : print "For thanks, {v_credits}." ;
      : print "For a more detailed history, {v_history}." ;
      : print "For verbs to use, {v_verbs}." ;
      : print "Note {cdsa} uses a color-coding scheme. Stuff to change is <lavender<#f4f>>, while stuff that's just for amusement is <grey<#ccc>>." ;
   }
   : match "credits _;credit _" {
      : print "Thanks to the administrators Ruber Eaglenest and Alyshkalia for holding {ecomp}, as well as former administrators like JJ Guest and Duncan Bowsman." ;
      : print "Thanks to Chris Ainsley for creating Adventuron." ;
      : print "I had no time for testers, alas, but if you find a bug, you can get your name here!" ;
      : print "Thanks to <mazegenerator.net<#0f0>>[http://www.mazegenerator.net] for generating the maze seen in some graphics." ;
      : print "Blanket thanks to Python and its developers for scripts that helped me generate similar code (point scoring, etc.) quickly." ;
      : if (!warn_generic_credit) {
         : print "{note_once}you can see generic Adventuron credits with <*CREDITS<#f80>>." ;
         : set_true "warn_generic_credit" ;
      }
   }
   : match "help _;hint _;hints _" {
      : if (!warn_hints) {
         : print "{note_once}there are no explicit in-game hints beyond how to use {hint_item}." ;
         : print "Also, {cdsa} comes with a walkthrough, map and logic flow chart if you want spoilers." ;
         : if (!point_weak_link) {
            : print "You may wish to use {v_think} to receive progressive hints to the solution to the first puzzle." ;
         }
         : set_true "warn_hints" ;
         : done ;
      }
      : if (!ever_x_me) {
         : print "Before using hints, {xme} to see what your hint/help item is." ;
		 : done ;
      }
      : cycle key = "xnumhints" predictable_order = "true" skip_interval = "0" {
         : print "This help has five parts total and is game state agnostic, e.g. the cycle won't change. {xnum} should give you a number. You may be able to guess what it means, but if not, I will give progressive hints." ;
         : print "If {xnum}'s number is 0 or -1, you are done here." ;
         : print "The rest of the time, {xnum} gives a two-digit number." ;
         : print "{xnum} will give, in order, the number of letters in each word (tens digit = first, ones = second.) In one case, it gives a three-digit number." ;
         : print "Then the tens digit=first, hundreds digit=second." ;
         : print "This is necessary because one puzzle has a ten-letter word. But don't worry. It will be clued in other ways." ;
      }
      : done ;
   }
   : match "history _;version _;versions _" {
      : print "{cdsa} came about after {l_gsgr}, when I just said, what would happen if I had an {ecomp} entry with the leftovers from brainstorming {l_ut} and {l_wp}? Obviously some ideas were more amusingly spooky than others, and some words suggested themselves." ;
      : print "" ;
      : print "So {l_gsgr} formed, and I focused on features. During the stretch run for {l_ut} in late August, I had a flurry of ideas while procrastinating bug fixes and transcript reading. One of the transcripts was to blame, so to speak: two testers noticed an unintentional red herring in the Cell, Urban ... which might have been a better puzzle than the cell itself, though the cell had a better \"after\" room name. So that was my starting puzzle. Then soon after I saw something indicating the final puzzle. I had to go through with it!" ;
      : print "" ;
      : print "{cdsa} has more rooms than {l_gsgr} so it doesn't have bells and whistles, but I may add them post-comp. No way was I fitting everything in under four hours." ;
      : print "Leftovers from {cdsa} are found in {l_dadc}." ;
      : print "" ;
      : print "So, late August 2025, then the second half of September 2025." ;
   }
   : match "score _" {
      : print "You've currently scored {score} of {max_score} total points. There are no bonus points." ;
      : if (think_any > 0) {
         : print {( "You've also discovered {think_any} command" + (think_any > 1 ? "s" : "" ) + " you can re-use in the future with stronger preparation." )} ;
      }
      : if (!warn_score_header) {
         : print "{note_once}This is tracked in the upper left." ;
         : set_true "warn_score_header" ;
      }
   }
   : match "think _" {
      : if (is_at "wee_clink") {
         : if (!clink_think) {
            : print "{one_time}a small make-sure nag before starting. {v_think}ing here will progressively reveal hints as to the first action." ;
            : set_true "clink_think" ;
            : done ;
         }
         : cycle key = "firsthint" predictable_order = "true" skip_interval = "0" {
            : print "Standard commands won't help you leave, though WAITing may give an additional clue." ;
            : print "So does what you say to yourself following a parser error. (It will disappear once you've left the clink.)" ;
            : print "The game title and some things may seem odd. They are a clue of what to do in the wee clink." ;
            : print "Also, the titles of similar games if you type {v_about} then {v_history} may provide clues." ;
            : print "The {cdsa}. The {sosc}. But how do they fit in with the wee clink?" ;
            : print "You need to break up wee clink into two different words with the same sounds, in order. This will help you find somewhere not secure in your jail." ;
            : print "A <WEAK LINK<#f80>>, if you will. (This is the last hint. They will cycle until you leave here.)" ;
         }
         : done ;
      }
      : if (!got_think) {
         : print "There are no commands you figured out and will need to re-use in the future with stronger preparation." ;
         : done ;
      }
      : if (think_deem_unfit) {
         : print "You can <DEEM UNFIT<#f80>> the demon (fit) {once_deem_unfit} you have ways to slow down its jabber a bit." ;
      }
      : if (think_hi_strawberry) {
         : print "You can say <HI STRAWBERRY<#f80>> {once_hi_strawberry} you find where such an entity might be." ;
      }
      : if (think_thee_floored) {
         : print "You can say <THEE FLOORED<#f80>> to the thief lord {once_thee_floored} you have more leverage." ;
      }
      : if (think_great_ankhs) {
         : print "You can convert the gray tanks to <GREAT ANKHS<#f80>> {once_great_ankhs} you have {think_tank_detail}." ;
      }
   }
   : match "verbs _;verb _" {
      : print "{cdsa} uses a stripped-down parser. <TAKE<#0f0>> or <DROP<#0f0>> are disabled, as the game picks up and removes items for you as needed. {v_i} still shows them." ;
      : print "{cdsa} uses the four compass directions, along with <UP<#0f0>> and <DOWN<#0f0>> and <IN<#0f0>> and <OUT<#0f0>>. You need to <X<#0f0>>/<EXAMINE<#0f0>> some items you find. They're in your {v_inv}, which you can also see with <I<#0f0>>." ;
      : print "Meta-verbs include {v_about}, {v_credits}, and {v_verbs}. {v_score} shows the score, which is also in the upper right header." ;
      : print {( "All point-scoring commands have two words which you need to figure. {v_think} will " + (point_weak_link ? " walk you through this first puzzle, then later " : "" ) + "show any commands you figured but didn't have in-game prep for yet." )} ;
      : print "{v_hint} explains how to use {hint_item}." ;
      : print "TSTART/TSTOP start and stop a transcript." ;
   }
   : match "x me;x myself" {
      : if (!ever_x_me) {
         : print "Weird! You see {a_num} on your arm." ;
         : set_true "ever_x_me" ;
         : done ;
      }
      : print "There's {a_num} on your arm, which may or may not help you." ;
   }
   : match "x number;x umber" {
      : set_true "ever_x_me" ;
      : if (!ever_x_number) {
         : print "You don't remember getting a tattoo. And the name sure feels artificial, but hey, any help is better than no help. Plus you don't have to use it." ;
         : print "As you inspect it, it swirls around a bit." ;
         : print "{note_once} the number doesn't distinguish if you have the in-game help needed to solve a puzzle. I was close to 4 hours for {ecomp} Petite Mort when I implemented it. If you wish to understand how it works, type {v_hint}." ;
         : set_true "ever_x_number" ;
      } : else {
         : print "You watch the number swirl around a bit more." ;
      }
      : print "The current number is {clue_number}.";
      : if (clue_number == 0) {
         : print "So, nothing to do here." ;
      } : else_if (clue_number > 100) {
         : print "Weird it's so big. Twice as big as it should be." ;
      }
   }

   : match "weak _;walk _;off _;sly _;dart _;pawn _;gull _;arbor _;first _;great _;deem _;thee _;hi _" {
      : print "{custom_unknown_verb_error}" ;
      : done ;
   }

}

on_tick {
   : if (pawn_is_around) {
      : match "i _" {
         : print "The pawn (dicey) also skulks behind in the shadows, ready to do your bidding at the right time." ;
