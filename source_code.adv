   r_blob : string "<Blah, Bland Blob Land<#ccc>>" ;
   r_sasf : string "<Self-Arm-Sell Farm<#ff0>>" ;

   # the main strings
   r00_header : dynamic_string {( return_sly_maps ? "<A Cramp-Ache Ramp<#ff0>>" : point_sly_maps ? "{r_blob}" : "{puz_slime_apps}" )} ;
   r00_loc_desc : dynamic_string {( return_sly_maps ? "{sc_acar} leads up east. It's a bit steep, so climbing may be painful, but nothing too bad. Back north, there's the wall." :
      point_sly_maps ? "The slime has oozed away, but its toxicity hasn't. Come back in a bit to see what's there." :
      "Hidden {puz_slime_apps} occasionally fire out ooze blocking passage every way but back north." )} ;
   r00_nogo : dynamic_string {( "You can only go north" + (return_sly_maps ? " or east." : ".") )} ;

   r01_header : dynamic_string {( point_walk_round ? "<Wall, Crowned<#ff0>>" : "{puz_wall_crowned}" )} ;
   r01_loc_desc : dynamic_string {( return_thee_floored ? "You've been around and found a lot. You probably want to follow that new path onward and <IN<#0f0>>ward that you just discovered." : (( point_walk_round ? "" : "A {puz_wall_crowned}. ") + "There's no way or reason to go back into the wee clink, so you probably want to " + (point_walk_round ? "go north, east, south or down, where you found exits in the wall." : "find exits in the wall somehow.")) )} ;
   r01_nogo : dynamic_string {( point_walk_round ? "You can't go that way, but there are exits north, south, east and down." : "The {puz_wall_crowned} is blocking you. You need to find a way through. There must be at least one, if you look enough." + " {r01_noback}" )} ;
   r01_noback : string "You don't want to go back <IN<#0f0>>ward to the wee clink." ;

   r02_header : dynamic_string {( point_gull_flying ? "<Gulf, Lying<#ff0>>" : return_off_lame ? "{puz_gulf_lying}" : point_off_lame ? "{r_blob}" : "{puz_awe_flame}" )} ;
   r02_loc_desc : dynamic_string {( point_gull_flying ? "The gull, flying, showed you a path east." : return_off_lame ? "You're in a {puz_gulf_lying}. Like you can't really go anywhere further. Right? It seems to say you can only go back south, and that's that." : point_off_lame ? "It's barren here now. Maybe when you come back, you'll see more." : "An {puz_awe_flame} distracts you from any belief you can progress further. Well, technically, it's a bunch of flames that together make a huge one.\nThey're impressive but contained. At least you remember it's still safe back south. But you dare to dream of a way to make the flame look less impressive." )} ;
   r02_nogo : dynamic_string {( return_off_lame ? "You see no passage." : point_off_lame ? "You should come back and look for new exits once the land's healed from the flame." : "The awe-flame distracts you too much from finding new exits." )} ;

   r10_header : dynamic_string {( point_great_ankhs ? "<Great Ankhs<#ff0>>" : "{puz_gray_tanks}" )} ;
   r10_loc_desc : dynamic_string {( "This place is covered with " + ( point_great_ankhs ? "great ankhs" : "{puz_gray_tanks}" ) + ". It's too desolate everywhere except back west." )} ;
   r10_nogo : string "You can only go back west." ;

   r11_header : dynamic_string {( point_pawn_dicey ? "<Pond, Icy<#ff0>>" : "{puz_pond_icy}" )} ;
   r11_loc_desc : dynamic_string {( "Back west is the wall. There's also a forbidding passage down past the pond. " + (point_pawn_dicey ? "You've found a secret from the pond." : "Perhaps the {puz_pond_icy} holds a secret." ) )} ;
   r11_nogo : string "You can only go back west or down." ;

   r12_header : dynamic_string {( point_arbor_dens ? "<Arbor Dens<#ff0>>" : "{puz_our_burdens}" )} ;
   r12_loc_desc : dynamic_string {( ( point_first_attic ? "It's real nice and verdant here, but now you've figured the mystery of the fir (static), there's nothing left to do." : point_arbor_dens ? "There are a bit more trees now, but one particularly stands out. A {puz_fir_static}." : "It's bare and desolate here. You hear incorporeal whining \"Our burdens ... {puz_our_burdens} ...\"" ) + "\nThe only way back is west." )} ;
   r12_nogo : string "You can only go back west." ;

   r20_header : dynamic_string {( "<Sword-Or-Sore Door<#ff0>>" )} ;
   r20_loc_desc : dynamic_string {( "Hoo boy it's dark in here. You can only go back up." )} ;
   r20_nogo : string "You can only go back up." ;

   r21_header : dynamic_string {( point_deem_unfit ? "<Tomb Eek-Too-Meek<#ff0>>" : "<Tomb End-To-Mend<#ff0>>" )} ;
   r21_loc_desc : dynamic_string {( "This is where the maze ends. You can only go back north." + (point_deem_unfit ? "\nOkay, the {sc_ddda} is here, but yeah. No." : "") )} ;
   r21_nogo : string "You can only go back north." ;

   r22_header : dynamic_string {( "<Maze Which May Switch<#ff0>>" )} ;
   r22_loc_desc : dynamic_string {( "There's a maze to the south, though you can go back up, too." )} ;
   r22_nogo : string "The maze is south, or you can go back up." ;

   r_desc_sasf : dynamic_string {( (is_first_entered () ? "A bunch of anthropomorphic fruits sit around and regard you suspiciously. As if you're the weird one here. They say, are you looking for anybody? Our leader is busy right now. They won't just see anybody. We will tolerate you different types around here. But don't expect to make friends." : "The anthropomorphic fruits continue to wait around, still regarding you warily. None seems to be the leader. Who or where or what might that be?") + "\nYou don't feel you fit in here, but there's nothing to do where you came from. Maybe something you're carrying will help you start negotiations." )} ;

   exits_err : dynamic_string {(
      is_at "wee_clink" ? "You need to figure a puzzle to leave, and when you do, you'll automatically go out, no directions needed." :
      is_at "r00" ? "{r00_nogo}" :
      is_at "r01" ? "{r01_nogo}" :
      is_at "r02" ? "{r02_nogo}" :
      is_at "r10" ? "{r10_nogo}" :
      is_at "r11" ? "{r11_nogo}" :
      is_at "r12" ? "{r12_nogo}" :
      is_at "r20" ? "{r20_nogo}" :
      is_at "r21" ? "{r21_nogo}" :
      is_at "r22" ? "{r22_nogo}" :
      is_at "sell_farm" ? "Nothing beyond here. You must call the fruits' leader to you." :
      "You can't go that way. I should have a non-default message." )} ;

   noguess : dynamic_string {( counter == 5 ? "<(A)<#888>>" : "<(unknown)<#888>>" )} ;
}

integers {

   # placemarking integers
   counter : integer "0" ;
   last_first_puzzle_score : integer "0" ;
   part_word_score : dynamic_integer {( ((collection_get{ collection -> ("got_first") index -> ( counter ) } == "true") ? 1 : 0) + ((collection_get{ collection -> ("got_second") index -> ( counter ) } == "true") ? 2 : 0) )} ; # this is a really bad hack, but I couldn't find a concise way to code things. Maybe I could use a dynamic boolean.
   tempint : integer "0" ;

   score : integer "0" ;
   max_score : integer "13" ;

   thought_right : dynamic_integer {(
      (think_great_ankhs ? 1 : 0) +
      (think_deem_unfit ? 1 : 0) +
      (think_thee_floored ? 1 : 0) +
      (think_high_strawberry ? 1 : 0)
  )} ;

   clue_number : dynamic_integer {(
      is_at "wee_clink" ? 44 :
      is_carried "aid_art" ? 14 :
      is_carried "me_code" ? 410 :
      is_at "r00" ? (point_sly_maps ? 0 : 34) :
      is_at "r01" ? (point_walk_round ? 0 : 45) :
      is_at "r02" ? (!point_off_lame ? 34 : (!return_off_lame ? -1 : (!point_gull_flying ? 46 : 0)) ) :
      is_at "r10" ? (point_great_ankhs ? 0 : 55) :
      is_at "r11" ? (point_pawn_dicey ? 0 : 45) :
      is_at "r12" ? (point_first_attic ? 0 : point_arbor_dens ? 55 : 54) :
      is_at "r20" ? (point_thee_floored ? 0 : 47) :
      is_at "r21" ? (point_deem_unfit ? 0 : 45) : # r22 has nothing specific to it, so no clues. The Aid Art is accounted for above.
      0
      )} ;
}

booleans {
   # game-state booleans (mostly did the player use a certain command) in all EctoComp games
   in_number_choice : boolean "true" ;
   inventory_note : boolean "false" scope="survivor" ;
   noun_based_noted : boolean "false" scope="survivor" ;
   score_note : boolean "false" scope="survivor";
   showed_first_note : boolean "false" scope="survivor" ;
