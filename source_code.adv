subroutines {
   add_point : subroutine {
      : print "<Your score has just gone up by a point!<#ff0>>" ;
      : increment "score" ;
      : update_graphic ;
   }

   check_odd_verbs : subroutine {
      : match "xyzzy _" {
         : print "A hollow voice intones \"Our gaol: argh, ail!\" It's so spooky, you don't dare quibble over details of pronunciation." ;
         : done ;
      }
      : mask {
         : match "load -" {
            : print "You hear a low \"Do load, ooh!\"" ;
         }
         : match "restore -" {
            : print "You hear a tune. It could be called a re-load reel/ode, I guess." ;
         }
         : match "save -" {
            : print "A ghostly apparition with \"Save? Illun', Say!\" Villain written on its front taunts your very reasonable precaution. But you don't need to listen to such meanies." ;
         }
      }
   }

   check_options : subroutine {
      : match "hone aye;hone eye" {
         : print {( "Available exits are " + (opt_hone_aye ? "already" : "now") + " honed to ones you still need to use to win." )} ;
         : if (clue_number == 0 && !opt_hone_aye) {
            : print "Since you're choosing this option, and you're done here, I'll make a note of that. You can exit, but you can't come back." ;
         }
         : set_true "opt_hone_aye" ;
      }
      : match "ho nigh" {
         : print {( "Exits are " + (opt_hone_aye ? "already" : "now") + " all available, even ones you don't need to revisited." )} ;
         : set_false "opt_hone_aye" ;
      }
      : match "hone _" { #noobfus
         : print "{command_stickler} {vo_hone_aye}.";
      }
      : match "ho _" { #noobfus
         : print "{command_stickler} {vo_ho_nigh}.";
      }
      : gosub "opt_see_seal" ;
   }

   opt_see_seal : subroutine {
      : match "see lol" {
         : print {( "{opt_txt_see_seal} is " + (opt_see_lol ? "already" : "now") + " on." + (see_lol_known ? "" : " {vo_seal_all} turns it off.") )} ;
         : set_true "opt_see_lol" ;
         : set_true "see_lol_known" ;
         : done ;
      }
      : match "seal all" {
         : print {( "{opt_txt_see_seal} is " + (opt_see_lol ? "now" : "already") + " off." + (see_lol_known ? "" : " {vo_see_lol} turns it on.") )} ;
         : set_false "opt_see_lol" ;
         : set_true "see_lol_known" ;
         : done ;
      }
      : if (see_lol_known) {
         : match "see _" {
            : print "{command_stickler} {vo_see_lol}." ;
            : done ;
         }
         : match "seal _" {
            : print "{command_stickler} {vo_seal_all}." ;
            : done ;
         }
      }
   }

   }

   custom_verb_general_checks : subroutine { # this is so obfus.py can "see" any odd verbs that might be used
      : if (pawn_is_around) {
         : match "_ pawn" {
            : print "The pawn is keeping low until the moment it needs to attack. You don't need to consult with it." ;
            : done ;
         }
      }
      : if (is_carried "aid_art") {
         : match "eh _" {
            : print "You're on the right track. No need for an interjection. It's something simpler. In fact, a word so simple, Adventuron doesn't care if it's there or not." ;
         }
         : match "dart -" {
            : if (point_a_dart) {
               : print "Already done!" ;
               : done ;
            }
            : destroy "aid_art" ;
            : pocket "dart" ;
            : print "The aid art doesn't have explicit instructions on how to create a dart, but you suppose that's for the best. People shouldn't have too-easy access to lethal weapons. It takes a while to figure what to do, but thanks to your earlier wordsmithing, you see methods others might have missed. Enough to create a dart!" ;
            : set_true "point_a_dart" ;
            : gosub "add_point" ;
            : done ;
         }
         : if (!point_a_dart) {
            : match "dart _;_ dart" {
               : print "You just technically need one word, due to how Adventuron processes certain words." ;
               : done ;
            }
         }
      }
      : if (is_carried "key_nigh" && is_carried "sly_maps") {
         : match "_ key;_ map;_ maps;_ sly;_ nigh;_ keen;_ aye;_ eye" {
            : print {( "You notice the key and the map come together to form a full maze. This could be useful " + (is_at "r22" ? "to explore the maze to the south!" : "somewhere else!") )} ;
            : set_false "showing_room" ;
            : update_graphic "s_maze_full" ;
            : set_true "showing_room" ;
            : set_true "progress_full_map" ;
            : done ;
         }
      }
      : if (is_carried "key_nigh") {
         : match "_ key;_ nigh" {
            : print "You see the bottom half of a maze." ;
            : set_false "showing_room" ;
            : update_graphic "s_maze_s" ;
            : set_true "showing_room" ;
            : done ;
         }
      }
      : if (is_carried "sly_maps") {
         : match "_ map;_ maps;_ sly" {
            : print "You see the top half of a maze." ;
            : set_false "showing_room" ;
            : update_graphic "s_maze_n" ;
            : set_true "showing_room" ;
            : done ;
         }
      }
      : if (point_thee_floored) {
         : match "high strawberry" {
            : print "You look around to find someone or something or somefruit to notify of the heist robbery, but this isn't the place." ;
            : set_true "think_high_strawberry" ;
            : collection_modify { collection = "tried_combo" index -> (12) content -> ("true") } ;
            : gosub "note_thinks" ;
            : done ;
         }
         : match "high _;_ strawberry" {
            : set_boolean var = "done_already" {( point_high_strawberry )} ; #okay, not really a test case
            : set_boolean var = "think_already" {( think_high_strawberry )} ;
            : set_integer var="tempint" "12" ;
            : set_false "first_half" ;
            : match "high _" {
               : set_true "first_half" ;
            }
            : gosub "reject_half" ;
            : done ;
         }
      }
   }

   meta_about : subroutine {
      : print "{this_g} is a simplified-parser game that requires two-word commands to solve puzzles. Some puzzles rely on others. A few have alternate spellings. {this_g} tracks when you've guessed one word right. It placed {placing} of 32 in the 2025 EctoComp Petite Mort division.{about_specific}" ;
      : print "" ;
      : print "{about_map_notes}" ;
      : print "" ;
      : print "For thanks, {vm_credits}." ;
      : print {( "For a more detailed history, {vm_history}.{about_spoiler_flag}" )} ;
      : print "For verbs to use, {vm_verbs}." ;
      : print "Note {this_g} uses a color-coding scheme. Stuff to change is <lavender<#f4f>>, while stuff that's just for amusement is <grey<#ccc>>." ;
   }

   meta_credits : subroutine {
      : print "Game-specific thanks:" ;
      : print "Thanks to <mazegenerator.net<#0f0>>[http://www.mazegenerator.net] for generating the maze seen in some graphics." ;
      : print "Thanks to <Leon W. Lin<#0f0>> for post-comp testing of the {vm_think} feature."
      : print "Thanks to <mazegenerator.net<#0f0>>[http://www.mazegenerator.net] for generating the maze seen in some graphics." ;
      : gosub "meta_credits_general" ;
   }

   meta_credits_general : subroutine {
      : print "<GENERAL THANKS<#f80>>:" ;
      : print "Thanks to the administrators <Ruber Eaglenest<#0f0>>[https://rubereaglenest.itch.io], <Alyshkalia<#0f0>>[https://alyshkalia.itch.io] and <Passerine<#0f0>>[https://passerine.itch.io] for holding {l_ectocomp}, as well as former administrators like JJ Guest and Duncan Bowsman." ;
      : print "Thanks to Chris Ainsley for creating <Adventuron<#0f0>>[https://adventuron.itch.io/]." ;
      : print "Blanket thanks to Python and its developers for scripts that helped me generate similar code (point scoring, etc.) quickly." ;
      : if (!warn_generic_credit) {
         : print "{note_once}you can see generic Adventuron credits with <*CREDITS<#f80>>." ;
         : set_true "warn_generic_credit" ;
      }
   }

   meta_options : subroutine {
      : print "{this_g} has two options." ;
      : print "{vo_hone_aye} and {vo_ho_nigh} toggle blocking passage to rooms you're done with." ;
      : print "{vo_see_lol} and {vo_seal_all} toggle whether {vd_think} will show specific words you've guessed right." ;
      : set_true "read_options_yet" ;
   }

   meta_think_conditionals : subroutine {
      : if (think_deem_unfit) {
         : print "You can {t_deem_unfit} the demon (fit) {here_r21} {once_deem_unfit} you have ways to slow down its jabber a bit." ;
      }
      : if (think_high_strawberry) {
         : print "You can say {t_hi_strawberry} {once_hi_strawberry} you've found where such an entity might be." ;
      }
      : if (think_thee_floored) {
         : print "You can say {t_thee_floored} to the thief lord {here_r20} {once_thee_floored} you have more leverage." ;
      }
      : if (think_great_ankhs) {
         : print "You can convert the gray tanks to {t_great_ankhs} {once_great_ankhs} you have {think_tank_detail}." ;
      }
   }

   meta_think_first : subroutine {
      : if (!is_at "wee_clink") {
         : return ;
      }
      : set_integer var="counter" "0" ;
         : if (part_word_score > last_first_puzzle_score) {
            : print {( "<NOTE<#ff0>>: you have figured out " + (part_word_score == 3 ? "<WEAK LINK<#f80>> with separate commands, so you can just type it in. If you're not sure why, continue to {vm_think} for the explanation." : (part_word_score == 1 ? "<WEAK<#f80>> as the first word." : "<LINK<#f80>> as the second word.") ) )} ;
			: if (part_word_score < 3) {
			   : print "So I'll cut off the game mechanics hints this time, though if you type {vm_think} again, they will proceed." ;
			}
            : set_integer var="last_first_puzzle_score" {( part_word_score )} ;
			: done ;
         }
         : if (!clink_think) {
            : print "{note_once}a small make-sure nag before starting. {vm_think}ing here will progressively reveal hints as to the first action and track if you got it partially right." ;
            : set_true "clink_think" ;
            : done ;
         }
         : cycle key = "firsthint" predictable_order = "true" skip_interval = "0" {
            : print "You'll need a non-standard command to leave. <WAIT<#0f0>>ing may give an additional clue." ;
            : print "So does what you say to yourself following a parser error. (It will disappear once you've left the clink.)" ;
            : print "The game title and some things may seem odd. They are a clue of what to do in the wee clink." ;
            : print "Also, the titles of similar games if you type {vm_about} then {vm_history} may provide clues." ;
            : print "In-game, you have the {this_g} and the {sc_sosc}. But how do they fit in with the wee clink?" ;
            : print "You need to break up {puz_wee_clink} into two different words with the same sounds, in order. This will help you find somewhere not secure in your jail." ;
            : print "A <WEAK LINK<#f4f>>, if you will. (This is the last hint. They will cycle again until you leave here. Then {vm_think} will track commands you figured but can't use yet.)" ;
         }
         : done ;
      }

   meta_think_main : subroutine {
      : set_integer var="counter" "0" ;
      : set_false "think_half_yet" ;
      : if (opt_see_lol) {
         : while (counter < max_score) {
            : if ((collection_get{ collection -> ("tried_combo") index -> ( counter )} == "false")) {
               : if (part_word_score > 0) {
                  : if (!think_half_yet) {
                     : print {( (thought_any ? "Also, here" : "Here") + " is what you know from partially correct guessing:" )} ;
                     : set_true "think_half_yet" ;
                  }
                  : print {( (part_word_score != 2 ? (collection_get{ collection -> ("first_words") index -> (counter) }) : "{noguess}") + " " + (part_word_score >= 2 ? (collection_get{ collection -> ("second_words") index -> (counter) }) : "{noguess}") + " " + (collection_get{ collection -> ("situations") index -> (counter) }) )} ;
                  : if (!solved_both_parts_nag && part_word_score == 3) {
                     : print "{note_once}You have solved something in parts, but you haven't typed the complete command yet. If it doesn't work once you do, {vd_think} will track it until it does." ;
                     : set_true "solved_both_parts_nag" ;
                  }
               }
            }
            : increment "counter" ;
         }
      }
      : if (!think_half_yet && !thought_any) {
         : print "There's nothing you've currently figured out only partially, whether it's the right phrase and the wrong time, or half of a phrase you need." ;
      }
   }

   note_thinks : subroutine {
      : if (!think_note_yet) {
         : print "{note_once}{vm_think} will track any command that will score points at a later time. Also, the status line in the upper left will show +1 or more, for commands you've guessed right but still need prep for." ;
         : set_true "think_note_yet" ;
      }
   }

   reject_half : subroutine {
      : if (done_already) {
         : print "That's half of something you already got to work." ;
      } : else_if (think_already) {
         : print "That's half of something you already guessed. {vd_think} will recall it." ;
      } : else {
         : print "You feel you must have one word right." ;
         : if (first_half) {
            : collection_modify { collection = "got_first" index -> (tempint) content -> ("true") ; }
         } : else {
            : collection_modify { collection = "got_second" index -> (tempint) content -> ("true") ; }
         }
         : if (!note_half_limitations) {
            : print "{note_once}{this_g} spells out half-right guesses with {vd_think} by default. This option can be disabled with {vo_seal_all} or enabled with {vo_see_lol}." ;
            : set_true "note_half_limitations" ;
         }
      }
      : set_false "done_already" ;
      : set_false "think_already" ;
   }

   see_seal_ops : subroutine {
      : match "see lol" {
         : print {( "{opt_txt_see_seal} is " + (opt_see_lol ? "already" : "now") + " off." + (see_lol_known ? "" : " {vo_seal_all} turns it off.") )} ;
         : set_true "opt_see_lol" ;
         : set_true "see_lol_known" ;
         : done ;
      }
      : match "seal all" {
         : print {( "{opt_txt_see_seal} is " + (opt_see_lol ? "now" : "already") + " off." + (see_lol_known ? "" : " {vo_see_lol} turns it on.") )} ;
         : set_false "opt_see_lol" ;
         : set_true "see_lol_known" ;
         : done ;
      }
      : if (see_lol_known) {
         : match "see _" {
            : print "{command_stickler} {vo_see_lol}." ;
            : done ;
         }
         : match "seal _" {
            : print "{command_stickler} {vo_seal_all}." ;
            : done ;
         }
      }

}

themes {
   my_theme : theme {
      status_bar {
         : header_text ;
         : dynamic_text "my_score" ;
      }
      theme_settings {
         parser_delay  = 0
      }
      colors {
         status_bar_pen   = 15
         status_bar_paper = #222
      }
      system_messages {
         i_cant_do_that = That's a valid command, but not right here and now.
         not_present = You can't see ${entity} here.
         ask_new_game                                   = Would you like to restart {this_g}?
         ask_quit                                       = \"Sign out. *SIGH*, nowt!\", you mutter under your breath.\"\nDo you wish to quit {this_g}?
         invalid_choice                                 = {yn_or_12_or_14}
         post_quit = I hope you enjoyed the experience. Pressing {key_enter} will restart {this_g}.
         unknown_verb                                   = {custom_unknown_verb_error}
         you_cant_go_that_direction                     = {exits_err}
         you_see_nothing_special = {nospecial_custom}
         you_see_nothing_special_2 = {nospecial_custom}
      }

   }
}

## Print the objective when the game starts

on_startup {
   : print "{this_g}: {l_ectocomp} 2025 Petite Mort game" ;
   : if (won_yet) {
      : if (!win_warn) {
         : print "Thanks for replaying, but this is a reminder to look at the other {l_ectocomp} entries, too! Whether or not it's over now!" ;
         : press_any_key ;
      }
   } : else {
      : print "{note_once}whenever the blinking yellow cursor appears, push {key_enter} or left-click the mouse. This will usually happen to break up long chunks of narrative text." ;
   }
   : press_any_key ;
   : print "Halloween. You want to avoid kids trick-or-treating. You cut through a park you've never seen before." ;
   : update_graphic "s_start_1" ;
   : print "No-spark-knows park? It's dark in there ... but wait ... there's a light ahead, or something giving light ..." ;
   : press_any_key ;
   : update_graphic "s_start_2" ;
   : print "A huge glowing ark appears before you. A voice intones: CHEZ DARK SHADE ARK. You feel compelled to enter ..." ;
   : press_any_key ;
   : print "And when you do, unseen hands grab you. \"The first visitor through the portal! They don't look suspicious at all, but remember, it's the people we least suspect that are ruining things!\"" ;
   : print "You black out and wake up in ...";
   : press_any_key ;
   : clear_screen;
}

## Execute this every time a player moves or redescribes a location

on_describe {
   : if (!intro_shown_yet) {
      : print "{note_once}{this_g} uses a reduced parser. {vm_verbs} details them, though standard starting commands like {vm_about} and {xme} may help you get started." ;
      : set_true "intro_shown_yet" ;
   }
}

## this main subroutine tests metaverbs.
## custom_verb_general_checks has special custom verbs e.g. for an item you can carry around

on_command {
   : match "i _" {
      : if (carried() == 0) {
         : if (!picked_anything_up) {
            : print "You've found nothing yet." ;
         } : else_if (finished_with_items) {
            : print "You're carrying nothing, but you need nothing." ;
         } : else {
            : print "Everything you've found, you've used." ;
         }
         : done ;
      }
   }
   : match "take _;drop _" {
      : if (!take_drop_warn) {
         : print "You never need to take or drop anything in {this_g}. Everything is implicitly taken or destroyed as used." ;
         : set_true "take_drop_warn" ;
      } : else {
         : print "Take and drop aren't used in {this_g}." ;
      }
      : done ;
   }
   : match "z _;wait _" {
      : print "\"Loaf, right? Lo, fright!\"" ;
      : if (!wait_yet) {
         : print {( "{note_once}This is just a silly general response" + ( is_at "wee_clink" ? ", though it does provide a clue how to get out of here, in its own way" : "" ) + ". Waiting gives no penalty or benefit." )} ;
         : set_true "wait_yet" ;
      }
   }
   : match "map _;m _" {
      : set_true "showing_map" ;
      : clear_screen ;
      : update_graphic "m_blank" ;
      : if (!first_map_note) {
         : print "{note_once}You can mouse-click or hit {key_enter} to return to the game. Arrows indicate one-way paths, and red lines show ones you don't need any more, if {vo_hone_aye} is active." ;
         : set_true "first_map_note" ;
      }
      : press_any_key ;
      : set_false "showing_map" ;
      : redescribe ;
   }

   : gosub "custom_verb_general_checks" ;
   : gosub "check_options" ;
   : gosub "opt_see_seal" ;

   : match "about _" {
      : gosub "meta_about" ;
   }

   : match "credits _;credit _" {
      : gosub "meta_credits" ;
      : done ;
   }

   : match "help _;hint _;hints _" {
      : if (!warn_hints) {
         : print "{note_once}there are no explicit in-game hints beyond how to use {hint_item}." ;
         : print "Also, {this_g} comes with a walkthrough, map and logic flow chart if you want spoilers." ;
         : if (!point_weak_link) {
            : print "You may wish to use {vm_think} to receive progressive hints to the solution to the first puzzle." ;
         }
         : set_true "warn_hints" ;
         : done ;
      }
      : if (!ever_x_me) {
         : print "Before using hints, {xme} to see what your hint/help item is." ;
		 : done ;
      }
      : cycle key = "xnumhints" predictable_order = "true" skip_interval = "0" {
         : print "This help has five parts total and is game state agnostic, e.g. the cycle won't change. {xnum} should give you a number. You may be able to guess what it means, but if not, I will give progressive hints." ;
         : print "If {xnum}'s number is 0 or -1, you are done here." ;
         : print "The rest of the time, {xnum} gives a two-digit number." ;
         : print "{xnum} will give, in order, the number of letters in each word (tens digit = first, ones = second.) In one case, it gives a three-digit number." ;
         : print "Then the tens digit=first, hundreds digit=second." ;
         : print "This is necessary because one puzzle has a ten-letter word. But don't worry. It will be clued in other ways." ;
      }
      : done ;
   }
   : match "history _;version _;versions _" {
      : print "{this_g} came about after {l_gsgr}, when I just said, what would happen if I had an {l_ectocomp} entry with the leftovers from brainstorming {l_ut} and {l_wp}? Obviously some ideas were more amusingly spooky than others, and some words suggested themselves." ;
      : print "" ;
      : print "So {l_gsgr} formed, and it was small but feature-focused. Then during the stretch run for {l_ut} and {l_ifcomp} in late August, I had a flurry of ideas while procrastinating bug fixes and transcript reading. Two of the testers were to blame, so to speak: they noticed an unintentional red herring in the <Cell, Urban<#f80>>." ;
      : print "In retrospect, it was probably a better puzzle than the cell itself. I could've changed the room name, but I said \"I'll save it for later,\"  not seriously believing I'd write another game. Then soon after I saw something that would make a perfect final puzzle. I had to go through with it!" ;
      : print "" ;
      : print "{this_g} had more rooms than {l_gsgr} so the EctoComp version didn't have bells and whistles, but I added them post-comp." ;
      : print "Leftovers from {this_g} are found in {l_dadc}. In fact, each of the three games had unique features for EctoComp that I merged into all of them post-comp." ;
      : print "" ;
      : print "Version 1 was written late August 2025, then the second half of September 2025, then published October 30, 2025 for EctoComp Petite Mort, where it placed {placing} of 32 entries." ;
      : print "Version 2 was published after EctoComp finished, on December 3, 2025." ;
      : print "Version 3 is due to be published by the end of 2025. It is the last major update planned." ;
      : print "" ;
      : print "If you've found this elsewhere than Itch.io, the original version should be at {l_cdsa}." ;
   }
   : match "opt _;opts _;option _;options _" {
      : gosub "meta_options" ;
      : done ;
   }
   : match "score _" {
      : print "You've currently scored {score} of {max_score} total points. There are no bonus points." ;
      : if (thought_right > 0) {
         : print {( "You've also discovered {thought_right_words} command" + (thought_right > 1 ? "s" : "" ) + " you can re-use in the future with stronger preparation." )} ;
      }
      : if (!warn_score_header) {
         : print "{note_once}score information is tracked in the upper right." ;
         : set_true "warn_score_header" ;
      }
   }
   : match "think _" {
      : gosub "meta_think_first" ;
      : gosub "meta_think_conditionals" ;
      : gosub "meta_think_main" ;
   }

   : match "verbs _;verb _" {
      : print "{this_g} uses a stripped-down parser. <TAKE<#0f0>> or <DROP<#0f0>> are disabled, as the game picks up and removes items for you as needed. {vm_i} still shows them." ;
      : print "{this_g} uses the four compass directions, along with <UP<#0f0>> and <DOWN<#0f0>> and <IN<#0f0>> and <OUT<#0f0>>. You need to <X<#0f0>>/<EXAMINE<#0f0>> some items you find. They're in your {vm_inv}, which you can also see with <I<#0f0>>." ;
      : print "Meta-verbs include {vm_about}, {vm_credits}, and {vm_verbs}. {vm_score} shows the score, which is also in the upper right header." ;
      : print {( "All point-scoring commands have two words which you need to figure. {vm_think} will " + (point_weak_link ? "" : " walk you through this first puzzle, then later " ) + "show any commands you figured but didn't have in-game prep for yet." )} ;
      : print "{vm_hint} explains how to use {hint_item}." ;
      : print "{vm_tstart_stop} start and stop a transcript." ;
      : print {( "{vm_options} shows two option commands that follow {this_g}'s mechanic." + (score == 0 && !read_options_yet ? " They may be spoilers or clues for the first puzzle." : "" ) )}  ;
   }
   : match "x me;x myself" {
      : if (!ever_x_me) {
         : print "Weird! You see {a_num} on your arm." ;
         : set_true "ever_x_me" ;
         : done ;
      }
      : print "There's {a_num} on your arm, which may or may not help you." ;
   }
   : match "x number;x umber" {
      : set_true "ever_x_me" ;
      : if (!ever_x_number) {
         : print "You don't remember getting a tattoo. And the name sure feels artificial, but hey, any help is better than no help. Plus you don't have to use it." ;
         : print "As you inspect it, it swirls around a bit." ;
         : set_true "ever_x_number" ;
      } : else {
         : print "You watch the number swirl around a bit more." ;
      }
      : if (clue_number == -1) {
         : print "Weird. Looks like there should be a number, but there isn't. Maybe come back later." ;
         : done ;
      }
      : print "The current number is {clue_number}.";
      : if (clue_number == 0) {
         : print "So, nothing to do here." ;
         : done ;
      } : else_if (clue_number > 100) {
         : print "Weird it's so big. Twice as big as it should be." ;
      }
      : if (blurry_num) {
         : print "It's a bit blurry, though. Perhaps you can't quite do anything yet." ;
      }
   }

   : match "weak _;walk _;off _;sly _;dart _;pawn _;gull _;arbor _;first _;great _;deem _;thee _;high _" { #obfuscation
      : print "{custom_unknown_verb_error}" ;
      : done ;
   }

   : match "floored _;eh _" { #obfuscation of non solutions
      : print "{custom_unknown_verb_error}" ;
      : done ;
   }

}

on_tick {
   : if (pawn_is_around) {
      : match "i _" {
         : print "The pawn (dicey) also skulks behind in the shadows, ready to do your bidding at the right time." ;
      }
   }
}

