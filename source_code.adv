   solved_both_parts_nag : boolean "false" scope="survivor" ;
   think_note_yet : boolean "false" scope="survivor" ;

   # THINK temporary booleans in all EctoComp games
   done_already : boolean "false" ;
   first_half : boolean "false" ;
   once_now_boolean : boolean "false" ;
   think_already : boolean "false" ;
   think_half_yet : boolean "false" ;

   # map booleans in all EctoComp games
   first_map_note : boolean "false" scope="survivor" ;
   show_overlay : boolean "true" ;
   showing_map : boolean "false" ;

   #####################booleans specific to this game
   # points and quasi-points
   first_point : dynamic_boolean {( point_weak_link )} ;
   point_weak_link : boolean "false" ;
   point_walk_round : boolean "false" ;
   point_off_lame : boolean "false" ;
   return_off_lame : boolean "false" ;
   point_sly_maps : boolean "false" ;
   return_sly_maps : boolean "false" ;
   point_a_dart : boolean "false" ;
   point_pawn_dicey : boolean "false" ;
   point_gull_flying : boolean "false" ;
   point_arbor_dens : boolean "false" ;
   point_first_attic : boolean "false" ;
   point_great_ankhs : boolean "false" ;
   point_deem_unfit : boolean "false" ;
   point_thee_floored : boolean "false" ;
   return_thee_floored : boolean "false" ;
   point_high_strawberry : boolean "false" ;

   progress_x_sly : boolean "false" ;
   progress_x_key : boolean "true" ;
   progress_full_map : boolean "false" ;
   progress_got_dart : boolean "false" ;
   progress_ankh_used : boolean "false" ;

   think_great_ankhs : boolean "false" ;
   think_deem_unfit : boolean "false" ;
   think_thee_floored : boolean "false" ;
   think_high_strawberry : boolean "false" ;

   thought_any : dynamic_boolean {( think_great_ankhs || think_deem_unfit || think_thee_floored || think_high_strawberry )} ;

   # puzzle readiness dynamic booleans
   ready_great_ankhs : dynamic_boolean {( point_pawn_dicey && point_a_dart )}
   blurry_num: dynamic_boolean {( !is_carried "aid_art" && (
      (is_carried "me_code" && !is_at "sell_farm") ||
      (is_at "r20" && !point_deem_unfit) ||
      (is_at "r21" && !point_first_attic) ||
      (is_at "r10" && !ready_great_ankhs)
      )
   )} ;

   pawn_is_around : dynamic_boolean {( point_pawn_dicey && !point_great_ankhs )} ;

   at_end : dynamic_boolean {( is_at "sell_farm" && point_thee_floored )} ;

   r00_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r01" && point_great_ankhs )} ;
   r02_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r01" && point_first_attic )} ;
   r10_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r00" && point_great_ankhs )} ;
   r11_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r01" && point_thee_floored )} ;
   r12_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r02" && point_first_attic )} ;
   r20_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r11" && point_thee_floored )} ;
   r21_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r22" && point_deem_unfit )} ;
   r22_hone_block : dynamic_boolean {( opt_hone_aye && is_at "r01" && point_deem_unfit )} ;

   # option(s)
   opt_hone_aye : boolean "false" ;

   # map rendering booleans
   showing_room : boolean "true" ;

   # flags for one time notes, etc.
   warn_hints : boolean "false" ;

   intro_shown_yet : boolean "false" ;
   ever_x_me : boolean "false" ;
   ever_x_number : boolean "false" ;
   clink_think : boolean "false" ;

   # inventory emptiness tracking
   picked_anything_up : dynamic_boolean {( point_sly_maps || point_off_lame )} ;
   finished_with_items : dynamic_boolean {( score >= max_score - 1 )} ;
}

vocabulary {
   # standard verbs
   : verb / aliases = [gt, goto]
   : verb / aliases = [in, enter]
   : verb / aliases = [out, exit]

